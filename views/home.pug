extends _layout
  
block content
  include _nav

  #indexWrapper.container-center.section
    h4 WELCOME TO pickME!

block additional_scripts
  script.
    $(document).ready(function(){  
      var contacts = {};
      "#{session.contacts}".split(",").forEach(function(contact) {
        contacts[contact] = null;
      });
      $('.autocomplete').autocomplete({
        data: contacts,
      });
    });